{
  "manifest_version": 3,
  "name": "Estimated Calculator",
  "version": "1.0.0",
  "description": "Chrome extension для оценки задач по формуле Pert, с личными коэффициентами",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "storage"
  ]
}
